{
  "manifest_version": 2,
  "name": "GitHub-Markdown-TOC",
  "description": "a markdown toc tool for GitHub Article",
  "version": "0.0.1",
	"content_scripts":
	[
		{
			"matches": ["<all_urls>"],
			"js": [ "js/content-script.js"],
			"css":["css/content.css"],
			"run_at": "document_start"
		}
	],
	"background":{
		"script":["js/background.js"]
	},
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  }
}